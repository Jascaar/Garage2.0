@model IEnumerable<Garage2._0.Models.VehicleType>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfVehicle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpacesNeededDividend)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpacesNeededDivisor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SpacesNeeded)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeOfVehicle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpacesNeededDividend)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpacesNeededDivisor)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SpacesNeeded)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) | 
            @Html.ActionLink("Details", "Details", new { id = item.Id })
            @Html.ActionLink("Delete", "Delete", new { id = item.Id }, new { @class = "btn btn-danger" })
        </td>
    </tr>
}

</table>
